/**app.wxss**/
page {
  background-color: #f0f0f0;
  padding-bottom:50px;
}
view, text, form, image, navigator, input, button, scroll-view, checkbox-group, page {
  box-sizing: border-box;
}
.text-hinde {
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
/*搜索列表  */
.search-list-wrap-parent {
  position: relative;
  width: 100%;
  height: calc(100% - 238rpx);
}
.search-list-wrap-patrianglerent scroll-view {
  position: relative;
  width: 100%;
  height: 100%;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
/*横向滚动条  */
.transverse-box {
  position: relative;
  width: 100%;
  height: 90rpx;
  background: #fff;
}
.transverse-box checkbox-group {
  margin-left: 24rpx;
}
.transverse-box checkbox-group:first-child {
  margin-left: 0;
}
.transverse-box scroll-view {
  position: relative;
  width: 100%;
  height: 100%;
}
.transverse-box scroll-view .list-bot {
  position: relative;
  float: left;
  height: 100%;
  padding-left: 24rpx;
  padding-right: 24rpx;
  line-height: 90rpx;
  white-space: nowrap;
}
.transverse-box scroll-view .list-bot label {
  display: inline-block;
  margin-left: 24rpx;
  font-size: 15px;
  color: #999;
}
.transverse-box scroll-view .list-bot label:first-child {
  margin-left: 0;
}
.transverse-box scroll-view text {
  display: inline-block;
  margin-left: 24rpx;
  font-size: 15px;
  color: #999;
}
.transverse-box scroll-view text.iconfont {
  transform: rotate(180deg);
}
.transverse-box scroll-view text.active {
  color: #ff4240;
}
.transverse-box scroll-view text.active .iconfont {
  color: #ff4240;
}
.transverse-box scroll-view text:first-child {
  margin-left: 0;
}
.transverse-box scroll-view .list {
  display: inline-block;
  padding-left: 24rpx;
  padding-right: 24rpx;
  height: 100%;
}
/* .transverse-box swiper swiper-item {
  max-width: 40px;
} */
/*遮罩  */
.shado-box {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .2);
  z-index: 888888;
}
.shado-box .content {
  position: relative;
  width: 100%;
  height: 100%;
}
.shado-box .content .close-box {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}
.shado-box .content scroll-view {
  position: relative;
  width: 100%;
  height: 100%;
  padding-top: 10rpx;
}
.shado-box .content .tag-box {
  position: relative;
  width: 100%;
  height: auto;
  padding-bottom: 10rpx;
  padding-top: 10rpx;
  background: #fff;
}
.shado-box .content .tag-box .tag {
  position: relative;
  width: 50%;
  height: auto;
  padding-left: 45rpx;
  padding-right: 45rpx;
  padding-top: 15rpx;
  padding-bottom: 15rpx;
  float: left;
}
.shado-box .content .tag-box.time .tag.timeMore {
  width: calc(100% - 60rpx);
}
.shado-box .content .tag-box .tag .content.active {
  background: #4f99ff;
  color: #fff;
}
.shado-box .content .tag-box .tag .content {
  position: relative;
  width: 100%;
  height: 70rpx;
  padding-left: 5rpx;
  padding-right: 5rpx;
  text-align: center;
  line-height: 70rpx;
  font-size: 15px;
  color: #999;
  border-radius: 5px;
  background: #f0f0f0;
}
.shado-box .content .tag-box.time .tag {
  width: calc(100% / 3 - 36rpx);
  margin-left: 24rpx;
  padding-left: 0;
  padding-right: 0;
}
.time-button-box {
  position: relative;
  width: 100%;
  height: 96rpx;
  border-top: 1px solid #d6e8fb;
}
.time-button-box .inner-button {
  position: relative;
  width: 50%;
  height: 100%;
  line-height: 96rpx;
  text-align: center;
  font-size: 15px;
  color: #999;
  float: left;
}
.time-button-box .inner-button.active {
  color: #4a96ff;
}
/*日历  */
.timeMore-box {
  position: relative;
  width: 100%;
  height: auto;
  background: #fff;
}
/*箭头  */
.arrow {
  display: inline-block;
  width: 10px;
  height: 10px;
  border-top: 1px solid #cbcbcb;
  border-right: 1px solid #cbcbcb;
  transform: rotate(45deg);
}
/*压缩边框  */
.border {
  position: absolute;
  left: 0;
  bottom: 0;
  width: calc(100%);
  height: 1px;
  transform: scaleY(0.5);
  background: #e5e5e5;
}
/*tab  */
.tab-wrap-mediate {
  width: 100%;
  height: auto;
  box-shadow: 0 8px 15px 2px #e6e6e6;
}
.tab-wrap-mediate .tab {
  position: relative;
  height: 80rpx;
  float: left;
  text-align: center;
  line-height: 80rpx;
  font-size: 15px;
  color: #999;
  background: #fff;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.tab-wrap-mediate .tab.hind {
  display: none;
}
.tab-wrap-mediate .tab.active {
  color: #4a96ff;
}
.tab-wrap-mediate .tab.active .triangle {
  border-top-color: #4a96ff;
}
.tab-wrap-mediate .tab .borderx {
  position: absolute;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  width: 1px;
  height: 68rpx;
  background: #ccc;
  transform: scaleX(0.5);
}
.tab-wrap-mediate .tab:last-child .borderx {
  display: none;
}
.momark {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  width: 300rpx;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  font-size: 15px;
  color: #fff;
  background: rgba(0, 0, 0, .4);
  border-radius: 5px;
}
/*搜索列表  */
.search-child-list-wrap {
  position: relative;
  width: 100%;
  height: auto;
  padding-top: 20rpx;
  padding-left: 23rpx;
  padding-right: 23rpx; 
}
.search-child-list-wrap .list {
  position: relative;
  width: 100%;
  height: auto;
  padding-left: 20rpx;
  padding-right: 20rpx;
  padding-bottom: 20rpx;
  margin-bottom: 20rpx;
  background: #fff;
  border-radius: 4px;
  overflow: hidden;
  box-shadow: 0 0 15px 2px #e6e6e6;
}
.search-child-list-wrap .list navigator {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}
.search-child-list-wrap .list .title {
  position: relative;
  width: 100%;
  height: 80rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
  margin-bottom: 20rpx;
  font-size: 16px;
  color: #666;
}
.search-child-list-wrap .list .title .name {
  width: 40%;
  height: 100%;
  text-align: left;
  line-height: 80rpx;
  float: left;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.search-child-list-wrap .list .title .time {
  width: 60%;
  height: 100%;
  line-height: 80rpx;
  text-align: right;
  float: left;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  color: #ccc;
}
.search-child-list-wrap .list .list-wrap {
  position: relative;
  width: 100%;
  height: auto;
}
.search-child-list-wrap .list .list-wrap .inner-wrap {
  width: 100%;
  height: auto;
  padding-left: 10rpx;
  padding-right: 10rpx;
  font-size: 14px;
  color: #999;
}
.search-child-list-wrap .list .list-wrap .inner-list {
  position: relative;
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  margin-bottom: 10rpx;
  overflow: hidden;
}
.search-child-list-wrap .list .list-wrap .inner-list:last-child {
  margin-bottom: 0;
}
.search-child-list-wrap .list .list-wrap .tag {
  position: absolute;
  right: 0;
  bottom: 0;
  width: auto;
  height: 48rpx;
  padding-left: 25rpx;
  padding-right: 25rpx;
  text-align: center;
  line-height: 48rpx;
  color: #999;
  font-size: 13px;
  border-radius: 25px;
  background: #ebebeb;
}
.search-child-list-wrap .list .list-wrap .tag.active {
  color: #509aff;
  background: #dcebff;
}
.search-child-list-wrap .list .list-wrap .tag.red {
  color: #ff3333;
  background: #ffd6d6;
}
/*搜索列表机构人员  */
.PepCard {
  position: relative;
  width: 100%;
  margin-top: 10px;
  background: #fff;
  box-sizing: border-box;
  border-radius: 4px;
  overflow: hidden;
}
.PepCard image {
  display: block;
  position: absolute;
  left: 25rpx;
  top: 30rpx;
  max-width: 150rpx;
  max-height: 150rpx;
  border-radius: 50%;
  background-size: cover;
}
.PepCard navigator {
  display: block;
  position: relative;
  width: 100%;
  min-height: 170rpx;
  padding: 32rpx 30rpx 10rpx 190rpx;
}
.PepCard .h3 {
  display: block;
  position: relative;
  width: 100%;
  margin: 0 0 10rpx;
  color: #333;
  font-weight: 400;
  overflow: hidden;
  text-overflow: ellipsis;
}
.PepCard .h3 .p {
  min-width: 1rem;
  max-width: 100%;
  height: 100%;
  float: left;
  margin: 0;
  font-size: 16px;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}
.PepCard .h3 .time {
  min-width: 50rpx;
  max-width: 180rpx;
  padding: 0 8rpx;
  border-radius: 2px;
  height: 20px;
  text-align: center;
  line-height: 20px;
  margin-left: 5px;
  color: #8798af;
  float: left;
  font-size: 12px;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}
.PepCard .h3 .time .i {
  font-style: normal;
  color: #478ae5;
  font-size: 15px;
}
.PepCard .no-bg {
  display: block;
  max-width: 100%;
  height: 20px;
  line-height: 20px;
  overflow: hidden;
}
.PepCard .h3 .p {
  min-width: 100rpx;
  max-width: 100%;
  height: 100%;
  margin: 0;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}
.PepCard .no-bg .p {
  display: inline-block;
  font-size: 15px;
  color: #8798af;
}
.PepCard .h3 .taglis {
  display: inline-block;
  min-width: 50rpx;
  max-width: 140rpx;
  padding: 0 .08rem;
  border-radius: 2px;
  height: 20px;
  text-align: center;
  line-height: 20px;
  margin-left: 5px;
  color: #fff;
  font-size: 12px;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}
.PepCard .no-bg .taglis {
  height: 100%;
  line-height: 20px;
  display: inline-block;
  border: 1px solid #478ae5;
  color: #478ae5;
  font-size: 12px;
  margin-left: 5px;
}
.PepCard .h4 {
  margin: 0 0 10rpx;
  font-size: 14px;
  color: #8798af;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  font-weight: 400;
}
.number-warp {
  position: relative;
  width: 100%;
  height: auto;
  padding-top: 15rpx;
  padding-bottom: 15rpx;
  overflow: hidden;
}
.number-warp .numbers {
  position: relative;
  width: calc(100% / 3);
  float: left;
  text-align: center;
}
.number-warp .numbers:after {
  position: absolute;
  top: -15rpx;
  right: 0;
  width: 1px;
  height: 150%;
  background: #f1f3f5;
  content: "";
}
.number-warp .numbers:last-child:after {
  display: none;
}
.number-warp:before {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 1px;
  background: #f1f3f5;
  content: "";
}
.number-warp .number {
  display: inline-block;
  width: 100%;
  padding-left: 5px;
  padding-right: 5px;
  color: #478ae5;
  font-size: 15px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.number-warp .name {
  display: inline-block;
  width: 100%;
  padding-left: 5px;
  padding-right: 5px;
  color: #8798af;
  font-size: 13px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
/*三角形  */
.triangle {
  display: inline-block;
  width: 0;
  height: 0;
  margin-left: 10rpx;
  border-left: 10rpx solid transparent;
  border-right: 10rpx solid transparent;
  border-top: 15rpx solid #ccc;
  z-index: -1;
}
/*司法局logo  */
.sfj-logo {
  position: absolute;

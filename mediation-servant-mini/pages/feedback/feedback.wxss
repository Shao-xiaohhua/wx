/* pages/feedback/feedback.wxss */
page {
  background: #fff;
}
.feedback-list-wrap {
  width: 100%;
  height: auto;
  padding-left: 24rpx;
  padding-right: 24rpx;
  background: #fff;
}
.feedback-list-wrap .list {
  position: relative;
  width: 100%;
  height: auto;
  line-height: 100%;
}
.feedback-list-wrap .list .arrow {
  position: absolute;
  right: 24rpx;
  top: 0;
  bottom: 0;
  margin: auto;
}
.feedback-list-wrap .list:last-child .border {
  display: none;
}
.feedback-list-wrap .list .tp {
  width: 170rpx;
  height: 100rpx;
  line-height: 100rpx;
  float: left;
  font-size: 15px;
  color: #333;
}
.feedback-list-wrap .list .tp.at {
  width: auto;
}
.feedback-list-wrap .list input {
  width: calc(100% - 170rpx);
  height: 100rpx;
  float: left;
  font-size: 15px;
  color: #999;
}
.feedback-list-wrap .list switch {
  float: right;
  margin-top: 15rpx;
}
.feedback-list-wrap .list navigator {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 888;
}
.feed-done {
  position:fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  color: #fff;
  border: none;
  background: #4895ff;
  border-radius: 0;
}
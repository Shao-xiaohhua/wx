/* pages/notice-bulletin/notice-bulletin.wxss */
page {
  position: relative;
  width: 100%;
  height: 100%;
}
scroll-view {
  position: relative;
  width: 100%;
  height: calc(100% - 158rpx);
  background: #f0f0f0;
}
.notice-list-wrap {
  position: relative;
  width: 100%;
  height: auto;
  padding-left: 24rpx;
  padding-right: 24rpx;
  padding-bottom: 80rpx;
}
.notice-list-wrap .list {
  position: relative;
  width: 100%;
  height: auto;
  margin-top: 20rpx;
}
.notice-list-wrap .list navigator {
  display: block;
  position: relative;
  width: 100%;
  height: auto;
  padding-left: 22rpx;
  padding-right: 22rpx;
  border-radius: 5px;
  background: #fff;
}
.notice-list-wrap .list .title {
  position: relative;
  padding-bottom: 25rpx;
  padding-top: 25rpx;
  line-height: 100%;
}
.notice-list-wrap .list .title .pull-left {
  font-size: 16px;
  color: #666666;
  font-weight: bold;
}
.notice-list-wrap .list .title .pull-right {
  font-size: 14px;
  color: #666666;
}
.notice-list-wrap .list navigator .content {
  width: 100%;
  height: auto;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  font-size: 15px;
  color: #999;
}
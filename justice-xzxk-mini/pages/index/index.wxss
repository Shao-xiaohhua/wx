/**index.wxss**/
.index-banner {
  position: relative;
  width: 100%;
  height: 240rpx;
  background: linear-gradient(to right, #5b6df9 , #5297fd);
}
.index-banner image {
  position: absolute;
  left: 0;
  bottom: 0;
  display: block;
  width: 100%;
  height: 180rpx;
}
.link-wrap {
  width: 100%;
  height: auto;
  background: #fff;
}
.link-wrap .link-box {
  position: relative;
  width: calc(100% / 3);
  height: auto;
  float: left;
  text-align: center;
}
.link-wrap .link-box.orang .iconfont {
  color: #fb9142;
}
.link-wrap .link-box.grren .iconfont {
  color: #6dc113;
}
.link-wrap .link-box.blue .iconfont {
  color: #43a7ea;
}
.link-wrap .link-box.blue-l .iconfont {
  color: #1cd3c7;
}
.link-wrap .link-box.blue-d .iconfont {
  color: #5a92f0;
}
.link-wrap .link-box.red .iconfont {
  color: #f75b57;
}
.link-wrap .link-box navigator {
  display: block;
  width: 100%;
  height: auto;
  padding-bottom: 60rpx;
  padding-top: 60rpx;
  font-size: 16px;
  color: #666666;
}
.link-wrap .link-box navigator .iconfont {
  font-size: 40px;
  line-height: 100%;
}
.link-wrap .link-box navigator .name {
  padding-top: 30rpx;
  font-size: 15px;
  line-height: 100%;
}
.link-wrap .link-box:nth-child(3n) .border-right {
  display: none;
}
.index-btn-wrap {
  width: 100%;
  height: auto;
  padding: 40rpx 30rpx 0 30rpx;
}
.index-btn-wrap navigator {
  display: block;
  width: 100%;
  height: auto;
  padding-top: 25rpx;
  padding-bottom: 25rpx;
  line-height: 100%;
  text-align: center;
  margin-bottom: 40rpx;
  color: #fff;
  border-radius: 4px;
  background: #5297fd;
}
.p {
  width: 100%;
  height: auto;
  color: red;
  display: block;
}
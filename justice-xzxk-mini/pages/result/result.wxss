/* pages/result/result.wxss */
.query-wrap {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: auto;
  padding: 30rpx;
  background: #fff;
}
.query-wrap .query {
  width: 100%;
  height: 88rpx;
  background: #f0f0f0;
  border-radius: 4px;
}
.query input {
  width: calc(100% - 100rpx);
  height: 100%;
  padding-left: 30rpx;
  font-size: 16px;
  color: #999;
}
.query .icon-warp {
  width: 100rpx;
  height: 100%;
  text-align: center;
  line-height: 88rpx;
  color: #999999;
}
.query .icon-warp .iconfont {
  font-size: 20px;
}
.list-warp {
  width: 100%;
  height: auto;
  margin-top: 20rpx;
}
.list-warp .list {
  width: 100%;
  height: auto;
  padding: 40rpx 40rpx 20rpx 40rpx;
  margin-bottom: 20rpx;
  background: #fff;
}
.list-warp .list .title {
  margin-bottom: 25rpx;
  font-size: 16px;
  color: #333;
}
.list-warp .list .type {
  margin-bottom: 20rpx;
  font-size: 14px;
  color: #999;
}
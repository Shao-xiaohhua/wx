/* pages/answer/answer.wxss */
.list-wrap {
  width: 100%;
  height: auto;
}
title .list {
  width: 100%;
  height: auto;
  margin-bottom: 20rpx;
}
.list-wrap .list .child-wrap {
  width: 100%;
  height: auto;
  padding-left: 30rpx;
  padding-right: 30rpx;
  padding-top: 15rpx;
  padding-bottom: 15rpx;
  background: #fff;
}
.list-wrap .list .child-wrap navigator {
  position: relative;
  display: block;
  width: 100%;
  height: 100%;
  padding-left: 60rpx;
}
.list-wrap .title {
  position: absolute;
  left: 0;
  top: 5rpx;
  width: 40rpx;
  height: 40rpx;
  text-align: center;
  line-height: 40rpx;
  font-size: 12px;
  color: #fff;
  border-radius: 4px;
  background: #5297fd;
}
.list-wrap .title.active {
  background: #ff753f;
}
.list-wrap .content {
  font-size: 16px;
  color: #333;
}
.list-wrap .content.active {
  color: #999;
}
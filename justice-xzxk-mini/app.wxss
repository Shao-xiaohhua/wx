/**app.wxss**/
page, view, text, image, navigator, scroll-view, picker, input {
  box-sizing: border-box;
}
page {
  background: #f5f5f5;
}
.pull-left {
  float: left;
}
.pull-right {
  float: right;
}
.btn-box {
  width: 100%;
  height: auto;
  padding-left: 30rpx;
  padding-right: 30rpx;
  text-align: center;
}
.required-text {
  color: #fff;
}
.required-text.active {
  color: red;
}
.shaow-search-wrap {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .5);
  z-index: 888;
}
.shaow-search-wrap .search-wrap {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  width: 70%;
  height: 70%;
  background: #fff;
  border-radius: 4px;
  overflow: hidden;
}
.shaow-search-wrap .search-wrap .title {
  position: relative;
  width: 100%;
  height: 44px;
}
.shaow-search-wrap .search-wrap .input-wrap {
  width: 80%;
  height: 100%;
  float: left;
}
.bold-title {
  width: 100%;
  height: auto;
  padding: 60rpx 40rpx 0 40rpx;
  color: #333;
  font-size: 26px;
  background: #fff;
}
.shaow-search-wrap .search-wrap .input-wrap input {
  width: 100%;
  height: 100%;
  padding-left: 30rpx;
  font-size: 16px;
  color: #333;
}
.shaow-search-wrap .search-wrap .icon-wrap {
  width: 20%;
  height: 100%;
  text-align: center;
  line-height: 44px;
  color: #999;
  float: left;
}
.shaow-search-wrap .search-wrap .icon-wrap .iconfont {
  font-size: 60rpx;
}
.shaow-search-wrap .search-list-wrap {
  width: 100%;
  height: calc(100% - 44rpx);
  padding-left: 20rpx;
  padding-right: 20rpx;
  font-size: 18px;
  color: #999;
}
.shaow-search-wrap .search-list-wrap .list {
  position: relative;
  width: 100%;
  height: auto;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}
.btn-box .btn {
  position: relative;
  padding-top: 35rpx;
  padding-bottom: 35rpx;
  line-height: 100%;
  color: #fff;
  background: #5297fd;
  border-radius: 60px;
}
.clearfix:after, .clearfix:before {
  content: "";
  width: 100%;
  height: 0;
  clear: both;
  display: block;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.border-right {
  position: absolute;
  right: 0;
  top: 0;
  width: 1px;
  height: 100%;
  background: #eeeeee;
  transform: scaleX(.5);
}
.border-top {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 1px;
  background: #eeeeee;
  transform: scaleY(.5);
}
.border-left {
  position: absolute;
  left: 0;
  top: 0;
  width: 1px;
  height: 100%;
  background: #eeeeee;
  transform: scaleX(.5);
}
.border-bottom {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 1px;
  background: #eeeeee;
  transform: scaleY(.5);
}
.switch-wrap {
  position: absolute;
  top: -12px;
  right: 0;
  width: 70px;
  height: 25px;
  margin-top: 20rpx;
  color: #999;
  background: #f0f0f0;
  border-radius: 50rpx;
  display: inline-block;
}
.switch-wrap.active {
  background: #5297fd;
  color: #fff;
}
.switch-wrap .cont {
  width: 50%;
  height: 100%;
  line-height: 25px;
  text-align: center;
  font-size: 12px;
}
.switch-wrap .btn-bol {
  position: absolute;
  left: 0;
  top: -8px;
  width: 40px;
  height: 40px;
  border-radius: 50%;
  border: 1px solid #eeeeee;
  background: #fff;
  transition: transform .3s ease;
}
.switch-wrap.active .btn-bol {
  transform: translateX(31px);
}
/*多选下拉  */
.select-mour {
  position: relative;
  width: 100%;
  min-height: 44px;
  padding-bottom: 10px;
}
.select-mour .selected {
  width: 100%;
  height: auto;
}
.select-mour .selected .select-list {
  position: relative;
  padding-top: 5px;
  padding-bottom: 5px;
  padding-left: 5px;
  padding-right: 40px;
  margin-top: 7px;
  margin-right: 10px;
  background: #5297fd;
  border-radius: 6px;
  float: left;
  color: #fff;
}
.select-mour .select-list-wrap {
  position: absolute;
  left: 0;
  bottom: -200px;
  width: 100%;
  height: 200px;
  padding-left: 5px;
  padding-right: 5px;
  background: #fff;
  border-bottom: 1px solid #5297fd;
  border-left: 1px solid #5297fd;
  border-right: 1px solid #5297fd;
  border-bottom-right-radius: 4px;
  border-bottom-left-radius: 4px;
  z-index: 88888;
}
.select-mour .select-list-wrap .list {
  width: 100%;
  padding-top: 12px;
  padding-bottom: 12px;
  border-bottom: 1px solid #ccc;
  color: #999;
}
.close-box {
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0;
  margin: auto;
  width: 25px;
  height: 25px;
}
.close-box .close-parent {
  position: relative;
  width: 100%;
  height: 100%;
}
.close-box .close-parent::after {
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  margin: auto;
  width: 15px;
  height: 1px;
  background: #fff;
  transform: rotate(45deg);
  content: "";
}
.close-box .close-parent::before {
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  margin: auto;
  width: 15px;
  height: 1px;
  background: #fff;
  transform: rotate(-45deg);
  content: "";
}

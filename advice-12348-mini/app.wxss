/* Submit Button */
button.submit-btn {
  margin: 15px;
  border-color: transparent;
  background-color: #00cc99 !important;
}
button.submit-btn:after {
  border: none;
}
button.submit-btn[disabled] {
  color: #fff;
  background-color: #ddd !important;
}
button.active-state {
  color: #fff !important;
  background-color: #00c090 !important;
}
/* Advice List */
.advice-list .weui-panel__hd {
  font-size: 14px;
}
.advice-list weui-media-box {
  display: block;
}
.advice-list .weui-media-box__title {
  text-overflow: inherit;
  white-space: inherit;
  font-size: 17px;
}
.advice-list .weui-media-box__info {
  display: flex;
  align-content: center;
  justify-content: space-between;
  padding-bottom: 0;
}
.advice-list .weui-media-box__info__meta {
  color: #888;
  font-size: 14px;
}
.advice-list .status {
  display: inline-block;
  margin-right: 10px;
  padding: 4px 6px;
  border-radius: 3px;
  color: #fff;
  background-color: #ddd;
}
.advice-list .tag {
  margin-right: 4px;
  color: #00cc99;
}

/* Loading  */
.loadingMore {
  padding: 10px 0;
  text-align: center;
  font-size: 12px;
  color: #999;
}
.loading {
  z-index: 100;
  width: 100%;
  height: 100%;
  background-color: #fff;
}
.spinners {
  margin: 0;
  padding: 0;
  list-style: none;
}
.spinners li {
  width: 15px;
  height: 15px;
  position: absolute;
  background-color: rgb(204,204,204);
  top: 45%;
  border-radius: 50%;
}
.spinners li:nth-child(1) {
  background-color: #db2828;
  animation: cssload-move 1.5s infinite cubic-bezier(0.2, 0.64, 0.81, 0.23);
}
.spinners li:nth-child(2) {
  background-color: #21ba45;
  animation: cssload-move 1.5s 172.5ms infinite cubic-bezier(0.2, 0.64, 0.81, 0.23);
}
.spinners li:nth-child(3) {
  background-color: #fbbd08;
  animation: cssload-move 1.5s 345ms infinite cubic-bezier(0.2, 0.64, 0.81, 0.23);
}
@keyframes cssload-move {
  0% {
    left: 0%;
  }
  100% {
    left: 100%;
  }
}

.userinfo-avatar {
  display: inline-block;
  width: 100%;
  height: 100%;
  background-size: 100% 100%;
}
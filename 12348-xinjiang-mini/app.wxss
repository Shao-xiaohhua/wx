/**app.wxss**/
page {
  background: #f5f5f5;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.padding-wrap {
  position: relative;
  width: 100%;
  height: auto;
  padding-left: 24rpx;
  padding-right: 24rpx;
}
.title-line {
  width: 100%;
  height: auto;
  padding-left: 18rpx;
  line-height: 100%;
  font-size: 16px;
  color: #333;
  border-left: 4rpx solid #0e76bc;
}
.title-line text {
  line-height: 100%;
}
/* iconfont  */
@font-face {
    font-family: 'iconfont';
    src: url('iconfont.eot');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('iconfont.ttf') format('truetype'),
        url('iconfont.svg#iconfont') format('svg');
    font-weight: normal;
    font-style: normal;
}
.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  /* -webkit-text-stroke-width: 0.2px; */
  -moz-osx-font-smoothing: grayscale;
}
.icon-notification_fill:before { content: "\e66a"; }

.icon-friendfamous:before { content: "\e6cb"; }

.icon-type-contract:before { content: "\e63a"; }

.icon-icon-message:before { content: "\e68d"; }

.icon-icon-right:before { content: "\e60d"; }

.icon-user-lawyer:before { content: "\e648"; }

.icon-org-notary:before { content: "\e646"; }

.icon-user-expertise:before { content: "\e64a"; }

.icon-org-aid:before { content: "\e64b"; }

.icon-map:before { content: "\e601"; }

.icon-icon-search:before { content: "\e600"; }

.icon-apply:before { content: "\e70d"; }

.icon-computer:before { content: "\e714"; }

.icon-icon-to-right:before { content: "\e756"; }

.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}

view, text, form, image, navigator, input, button, scroll-view, checkbox-group, page, textarea {
  box-sizing: border-box;
}
.pull-left {
  float: left;
}
.pull-right {
  float: right;
}
.btn-link {
  width: 100%;
  height: auto;
  padding: 25rpx; 
  line-height: 100%;
  text-align: center;
  font-size: 18px;
  color: #fff;
  background: #1788ce;
  border-radius: 4px;
}
.bd-bottom {
  position: relative;
  display: block;
}
.bd-bottom:before {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 1px;
  background: #eeeeee;
  content: "";
}
.btn-link.active {
  background: #cccccc;
}
.type-list-warp {
  width: 100%;
  height: auto;
}
.type-list-warp .title {
  width: 100%;
  height: 80rpx;
  padding-left: 35rpx;
  padding-right: 35rpx;
  line-height: 80rpx;
  background: #f5f5f5;
  font-size: 15px;
  color: #999;
}
.type-list-warp .list-wrap {
  width: 100%;
  height: auto;
  padding-left: 35rpx;
  padding-right: 35rpx;
  font-size: 15px;
  color: #666666;
}
.type-list-warp .list-wrap .list {
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  border-bottom: 1px solid #eee;
}
.type-list-warp .list-wrap .list .iconfont {
  display: none;
}
.type-list-warp .list-wrap .list.active {
  color: #0e76bc;
}
.type-list-warp .list-wrap .list.active .iconfont {
  display: block;
}
.type-list-warp .list-wrap .list:last-child {
  border-bottom: none;
}
.plaColor {
  color: #999;
  font-weight: 100;
}